{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "rules": {
    "no-console": 0,
    "eqeqeq": 1,
    "no-unused-vars": 1,
    "import/no-unresolved": 0,
    "jsx-a11y/img-has-alt": 0,
    "jsx-a11y/img-uses-alt": 0,
    "no-undef": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "quote-props": 0,
    "react/jsx-first-prop-new-line": 0,
    "react/jsx-indent": 0,
    "react/jsx-no-bind": 1,
    "react/no-multi-comp": 0,
    "react/sort-comp": 0,
    "react/prefer-stateless-function": 1,
    "react/prop-types": 0,
    "arrow-body-style": 0,
    "array-bracket-spacing": 1,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": 2,
    "comma-spacing": 0,
    "comma-dangle": 0,
    "comma-style": 2,
    "default-case": 0,
    "eol-last": 2,
    "func-names": 2,
    "func-style": [
      0,
      "declaration"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "key-spacing": 1,
    "linebreak-style": [
      0,
      "unix"
    ],
    "max-len": 0,
    "no-trailing-spaces": 1,
    "object-shorthand": 1,
    "object-curly-spacing": 1,
    "one-var-declaration-per-line": 2,
    "padded-blocks": 0,
    "prefer-const": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "new-cap": 0,
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 1,
    "no-new-object": 2,
    "no-restricted-syntax": 2,
    "no-spaced-func": 2,
    "no-underscore-dangle": 0,
    "no-var": 1,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-space-before-closing": 1,
    "semi": 1,
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "spaced-comment": 1,
  },
  "globals": {
    "Promise": false,
    "require": false,
    "__DEV__": false
  }
}
